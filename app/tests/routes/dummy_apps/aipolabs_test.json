{
    "name": "AIPOLABS_TEST",
    "display_name": "Aipolabs Test",
    "version": "1.0.0",
    "provider": "aipolabs",
    "description": "Aipolabs Test is a hypothetical application used to demonstrate how API schemas can be structured. It offers basic functionalities for testing purposes such as creating resources, retrieving data, and simulating user interactions. This API is useful for developers testing their integrations and workflows.",
    "server_url": "https://api.aipolabs.com",
    "logo": "https://example.com/aipolabs-logo.png",
    "categories": [
        "testcategory",
        "utility"
    ],
    "tags": [
        "testing",
        "simulation"
    ],
    "supported_auth_schemes": {
        "api_key": {
            "parameter_name": "X-Test-API-Key",
            "location": "header"
        },
        "http_basic": {
            "username": "test_username",
            "password": "test_password"
        },
        "oauth2": {
            "authorization_url": "https://aipolabs.com/oauth/authorize",
            "token_url": "https://aipolabs.com/oauth/token",
            "refresh_url": "https://aipolabs.com/oauth/refresh",
            "default_scopes": [
                "read",
                "write"
            ],
            "available_scopes": [
                "read",
                "write",
                "admin"
            ],
            "client_id": "your_client_id",
            "client_secret": "your_client_secret",
            "additional_parameters": [
                {
                    "name": "redirect_uri",
                    "required": true
                },
                {
                    "name": "state",
                    "required": false
                }
            ]
        }
    },
    "functions": [
        {
            "name": "AIPOLABS_TEST__HELLO_WORLD",
            "description": "Say hello to the world",
            "parameters": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "description": "The name of the person to greet"
                    },
                    "greeting": {
                        "type": "string",
                        "description": "The greeting message"
                    }
                },
                "required": ["name", "greeting"]
            }
        },
        {
            "name": "AIPOLABS_TEST__HELLO_WORLD_NESTED_ARGS",
            "description": "Say hello to the world with nested args",
            "parameters": {
                "type": "object",
                "properties": {
                    "person": {
                        "type": "object",
                        "description": "The person to greet",
                        "properties": {
                            "name": {
                                "type": "string",
                                "description": "The name of the person"
                            },
                            "title": {
                                "type": "string",
                                "description": "The title of the person"
                            }
                        },
                        "required": ["name", "title"]
                    },
                    "greeting": {
                        "type": "string",
                        "description": "The greeting message"
                    },
                    "location": {
                        "type": "object",
                        "description": "The location of the person",
                        "properties": {
                            "city": {
                                "type": "string",
                                "description": "The city of the location"
                            },
                            "country": {
                                "type": "string",
                                "description": "The country of the location"
                            }
                        },
                        "required": ["city", "country"]
                    }
                },
                "required": ["person", "greeting", "location"]
            }
        }
    ]
}
