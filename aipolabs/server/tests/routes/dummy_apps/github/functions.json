        "tags": ["issue"],
        "visibility": "public",
        "enabled": true,
        "protocol": "rest",
        "protocol_data": {
            "method": "POST",
            "path": "/repositories/{repository}/issues",
            "server_url": "https://api.github.com"
        },
        "parameters": {
            "type": "object",
            "properties": {
                "path": {
                    "type": "object",
                    "properties": {
                        "repository": {
                            "type": "string",
