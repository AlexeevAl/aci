                        "title": {
                            "type": "string",
                            "description": "The title of the pull request"
                        },
                        "metadata": {
                            "type": "object",
                            "properties": {
                                "author": {
                                    "type": "string",
                                    "description": "The author of the pull request"
                                },
                                "reviewer": {
                                    "type": "string",
