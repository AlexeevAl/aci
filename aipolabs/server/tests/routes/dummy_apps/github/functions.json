
[
    {
        "name": "GITHUB__CREATE_REPOSITORY",
        "description": "Create a new repository",
        "tags": ["repository"],
        "visibility": "public",
        "enabled": true,
        "protocol": "rest",
        "protocol_data": {
            "method": "POST",
            "path": "/repositories",
            "server_url": "https://api.github.com"
        },
        "parameters": {
            "type": "object",
            "properties": {
                "body": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string",
                            "description": "The name of the repository"
                        },
                        "description": {
                            "type": "string",
