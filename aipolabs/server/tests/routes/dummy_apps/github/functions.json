    {
        "name": "GITHUB__CREATE_PULL_REQUEST",
        "description": "Create a new pull request in a repository",
        "tags": ["pull_request"],
        "visibility": "public",
        "enabled": true,
        "protocol": "rest",
        "protocol_data": {
            "method": "POST",
            "path": "/repositories/{repository}/pulls",
            "server_url": "https://api.github.com"
        },
        "parameters": {
            "type": "object",
            "properties": {
                "path": {
                    "type": "object",
                    "properties": {
                        "repository": {
                            "type": "string",
                            "description": "The name of the repository"
                        }
                    },
                    "required": ["repository"],
                    "additionalProperties": false
                },
                "body": {
                    "type": "object",
                    "properties": {
                        "source_branch": {
                            "type": "string",
                            "description": "The name of the source branch for the pull request"
                        },
                        "target_branch": {
                            "type": "string",
                            "description": "The name of the target branch where the pull request will be merged"
